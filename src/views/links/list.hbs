          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <h4 class="m-0 font-weight-bold text-center">DATA TAMU</h4>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>No</th>
                      <th>Nama</th>
                      <th>Alamat</th>
                      <th>Waktu</th>
                      <th>Aksi</th>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <th>No</th>
                      <th>Nama</th>
                      <th>Alamat</th>
                      <th>Waktu</th>
                      <th>Aksi</th>
                    </tr>
                  </tfoot>
                  <tbody>
                  {{#each links}}
                    <tr>
                      <th>{{ ID }}</th>
                      <td>{{Nama}}</td>
                      <td>{{Alamat}}</td>
                      <td>{{ timeago Created_at }}</td>
                      <td><a href="/links/detail/{{ID}}" class="btn btn-warning">Detail</a></td>
                    </tr> 
           {{/each}}
                  </tbody>
                </table>
              </div>
            </div>
          </div>